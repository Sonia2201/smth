        String[] languages={"C", "C++", "C#", "Python", "Go", "Rust", "JavaScript", "PHP", "Swift", "Java"};

        int n = (int) (Math.random() * 1_000_000);

        System.out.println(n);

        int m=n*3;
        System.out.println(m);
        int b=m+21;
        System.out.println(b);

        int h=b+255;
        System.out.println(h);
        int r=h*6;
        System.out.println(r);


       /* n = Integer.MAX_VALUE;
