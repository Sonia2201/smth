        System.out.println(s.toString());

        */



        while (n > 0) {
            int l = n % 10;
            l=l+l;
            System.out.println(l);
           if(l>10) {
               l=l%10;
               break;

            }

            System.out.println("Willy-nilly, this semester I will learn " + languages[l]);


    }




    }


